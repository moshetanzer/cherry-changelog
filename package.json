{
  "name": "cherry-changelog",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@10.12.2",
  "description": "Interactive changelog generator with commit picker. Choose which commits to include in your changelog with multiple output formats (JSON, Markdown, HTML). Perfect for teams who want full control over their release notes.",
  "license": "MIT",
  "repository": "moshetanzer/cherry-changelog",
  "keywords": [
    "changelog",
    "git",
    "conventional-commits",
    "interactive",
    "cli",
    "markdown",
    "json",
    "html",
    "release-notes",
    "commit-picker",
    "versioning",
    "developer-tools",
    "workflow",
    "team-collaboration"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.mts"
      ]
    }
  },
  "bin": {
    "cherry-changelog": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --fix",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint",
    "release": "bumpp && pnpm publish",
    "test:versions": "installed-check -d --no-workspaces",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "inquirer": "^12.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@types/node": "latest",
    "@vitest/coverage-v8": "latest",
    "bumpp": "latest",
    "changelogithub": "latest",
    "eslint": "latest",
    "installed-check": "latest",
    "knip": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  }
}
